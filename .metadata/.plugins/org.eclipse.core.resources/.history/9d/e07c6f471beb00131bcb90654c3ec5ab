package com.hackbulgaria.corejava.oopfun;

public class Car {
    
    public Car() {
        super();
    }

    class Audi {
        @Override
        public String toString() {
            return "Audi@129129";
        }
    }
    
    class Opel {
        @Override
        public String toString() {
            return "Opel@239820";
        }
    }
    
    class Citroen {
        @Override
        public String toString() {
            return "Citroen@q2789";
        }
    }
    
    class Peugeot {
        @Override
        public String toString() {
            return "Peugeot@23890";
        }
    }
}
